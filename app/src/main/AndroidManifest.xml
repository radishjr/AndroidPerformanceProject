<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nekoproject.androidperformanceproject" >

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LagActivity"
            android:label="@string/title_activity_lag" >
        </activity>
        <activity
            android:name=".SmoothActivity"
            android:label="Smooth" >
        </activity>
        <activity
            android:name=".MemoryLeakActivity"
            android:label="@string/title_activity_memory_leak" >
        </activity>
    </application>

</manifest>
